<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.shytov.controller.ResetPassword2Controller">
    <children>
        <Label layoutX="155.0" layoutY="43.0" text="Восстановление пароля">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <Label layoutX="94.0" layoutY="141.0" text="Введите пароль">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <Label layoutX="41.0" layoutY="206.0" text="Подтвердите пароль">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <PasswordField fx:id="passwordField" layoutX="299.0" layoutY="141.0" prefHeight="35.0" prefWidth="233.0">
            <font>
                <Font size="14.0" />
            </font>
        </PasswordField>
        <PasswordField fx:id="confirmation" layoutX="300.0" layoutY="206.0" prefHeight="35.0" prefWidth="233.0">
            <font>
                <Font size="14.0" />
            </font>
        </PasswordField>
        <Button layoutX="408.0" layoutY="305.0" mnemonicParsing="false" onAction="#reset" text="Восстановить">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button layoutX="270.0" layoutY="305.0" mnemonicParsing="false" onAction="#back" text="Назад">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button layoutX="117.0" layoutY="305.0" mnemonicParsing="false" onAction="#exit" text="Выход">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Label fx:id="error" layoutX="293.0" layoutY="261.0">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </children>
</AnchorPane>